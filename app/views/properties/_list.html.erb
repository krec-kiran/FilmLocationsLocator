<!-- * code for List view -->

<div class="col-sm-9">
    <% @properties.each do |property| %>
      <% if property[:pic].blank? %>
          <div class="col-sm-3 list-group">
            <%= image_tag "http://placehold.it/600x400", class: "play" %>
          </div>
        <% else %>
          <div class="col-sm-3 list-group">
            <%= image_tag property.pic, class: "play" %>
          </div>
        <% end %>
    <% end %>
</div>

